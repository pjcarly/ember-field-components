{{#if (eq this.widgetName "mask")}}
  <Output::TextMask
    @value={{this.value}}
    @prefix={{@prefix}}
    @suffix={{@suffix}}
    @class={{@outputClass}}
    @mask={{this.mask}}
    @regex={{this.regex}}
    @options={{this.outputOptions}}
  />
{{else}}
  <Output::Text
    @value={{this.value}}
    @prefix={{@prefix}}
    @suffix={{@suffix}}
    @class={{@outputClass}}
    @options={{this.outputOptions}}
  />
{{/if}}