{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "dummy/tests/*": ["tests/*"],
      "dummy/*": ["tests/dummy/app/*", "app/*"],
      "@getflights/ember-field-components": ["addon"],
      "@getflights/ember-field-components/*": ["addon/*"],
      "@getflights/ember-field-components/test-support": ["addon-test-support"],
      "@getflights/ember-field-components/test-support/*": [
        "addon-test-support/*"
      ],
      "@getflights/ember-attribute-validations": [
        "node_modules/@getflights/ember-attribute-validations/addon"
      ],
      "@getflights/ember-attribute-validations/*": [
        "node_modules/@getflights/ember-attribute-validations/addon/*"
      ],
      "*": ["types/*"]
    },
    "experimentalDecorators": true
  },
  "exclude": ["node_modules/@getflights/**/*"],
  "include": [
    "app/**/*",
    "addon/**/*",
    "tests/**/*",
    "types/**/*",
    "test-support/**/*",
    "addon-test-support/**/*",
    "node_modules/@getflights/ember-attribute-validations/app",
    "node_modules/@getflights/ember-attribute-validations/addon"
  ]
}
