<div class="btn-group" role="group">
  {{#each selectOptions as |selectOption|}}
    <button {{action 'valueChanged' selectOption.value}} class="btn{{if (eq selectOption.value value) ' active'}}{{if options.btnClass (concat ' ' options.btnClass) ' btn-default'}}">
      {{selectoption-label selectOption}}
    </button>
  {{/each}}
</div>
