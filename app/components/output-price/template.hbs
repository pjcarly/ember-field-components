<span class={{this.computedClass}}>
  {{#if @prefix}}
    <span class="prefix">
      {{@prefix}}
    </span>
  {{/if}}
  <span class={{if (or value (eq value 0)) 'value' 'value empty'}}>
    {{number-format value fieldInformation.locale numberOptions}}
  </span>
  {{#if @suffix}}
    <span class="suffix">
      {{@suffix}}
    </span>
  {{/if}}
</span>