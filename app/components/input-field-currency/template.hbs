{{#if isButtonGroup}}
  {{input-button-group selectOptions=selectOptions value=value valueChanged=(action 'valueChanged') styleClass=styleClass inputId=inputId}}
{{else if isSelectSearch}}
  {{input-select-search selectOptions=selectOptions value=value valueChanged=(action 'valueChanged') styleClass=styleClass inputId=inputId}}
{{else}}
  {{input-select selectOptions=selectOptions value=value isRequired=required valueChanged=(action 'valueChanged') styleClass=styleClass none=none inputId=inputId}}
{{/if}}
