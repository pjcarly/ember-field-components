{{#if (eq this.widgetName 'bootstrap')}}
  {{input-time-bootstrap value=value valueChanged=(action 'valueChanged') placeholder=placeholder prefix=prefix suffix=suffix styleClass=styleClass showButton=showButton inputId=inputId}}
{{else}}
  <InputTime @value={{this.value}} @valueChanged={{action (mut this.value)}} @preSetHook={{this.preSetHook}} @placeholder={{@placeholder}} @prefix={{@prefix}} @suffix={{@suffix}} @class={{@inputClass}} @inputId={{@inputId}} @options={{this.inputOptions}} />
{{/if}}
