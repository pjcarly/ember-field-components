<span class={{this.computedClass}}>
  {{#if (and @value @prefix)}}
    <span class="prefix">
      {{@prefix}}
    </span>
  {{/if}}
  <span class={{if this.selectedLabels "value" "value empty"}} ...attributes>
    {{#if this.selectedLabels}}
      <ul>
      {{#each @value as |singleValue|}}
        <li>{{singleValue}}</li>
      {{/each}}
      </ul>
    {{/if}}
  </span>
  {{#if (and @value @suffix)}}
    <span class="suffix">
      {{@suffix}}
    </span>
  {{/if}}
</span>