<select {{action 'valueChanged' on='change'}} class={{inputStyleClass}} id={{inputId}} disabled={{disabled}} multiple>
  {{#if showNone}}
    {{option-none selected=noneSelected disabled=noneDisabled label=noneLabel}}
  {{/if}}
  {{#each selectOptions as |selectOption|}}
    {{option-selected selectOption computedValue}}
  {{/each}}
</select>
