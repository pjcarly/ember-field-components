{{#if isResizeable}}
  {{input-textarea-resizeable value=value valueChanged=(action 'valueChanged') placeholder=placeholder styleClass=styleClass inputId=inputId}}
{{else}}
  <InputTextarea @value={{this.value}} @valueChanged={{action (mut this.value)}} @preSetHook={{this.preSetHook}} @placeholder={{@placeholder}} @class={{@inputClass}} @inputId={{@inputId}} />
{{/if}}
