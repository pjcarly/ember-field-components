{{#if isButtonGroup}}
  {{input-button-group selectOptions=selectOptions value=value valueChanged='valueChanged' styleClass=styleClass}}
{{else if isSelectSearch}}
  {{input-select-search selectOptions=selectOptions value=value valueChanged='valueChanged' styleClass=styleClass}}
{{else}}
  {{input-select selectOptions=selectOptions value=value valueChanged='valueChanged' styleClass=styleClass}}
{{/if}}
